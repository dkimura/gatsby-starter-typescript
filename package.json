{
  "name": "gatsby-starter-typescript",
  "description": "Gatsby TypeScript starter",
  "license": "MIT",
  "author": "Daichi Kimura <dkimura@bdd.xyz>",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "lint": "eslint .",
    "serve": "gatsby serve",
    "start": "gatsby develop"
  },
  "dependencies": {
    "gatsby": "2.20.16",
    "gatsby-link": "2.3.2",
    "gatsby-plugin-react-helmet": "3.2.1",
    "gatsby-plugin-typegen": "2.0.0",
    "gatsby-plugin-typescript": "2.3.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@types/node": "13.11.1",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-helmet": "5.0.15",
    "eslint": "6.8.0",
    "eslint-config-globis": "0.3.1",
    "husky": "4.2.5",
    "lint-staged": "10.1.3",
    "prettier": "1.19.1",
    "prettier-package-json": "2.1.3",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  },
  "keywords": [
    "gatsby, typescript"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "package.json": [
      "prettier-package-json --write"
    ],
    "*.graphql": [
      "prettier --write"
    ]
  }
}
